[run]
source = app, cli.py, scripts
omit = 
    app/watcher_demo.py
    app/gui/*
    build/*
    shibokensupport/*
    */tests/*
    */__pycache__/*
    test_script.py
    signature_bootstrap.py
    # Exclude test files that don't run in CI
    tests/test_gui_functional.py
    tests/test_gui_skeleton.py
    tests/test_packaging.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod 